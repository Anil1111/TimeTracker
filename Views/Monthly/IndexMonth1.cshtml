@model IEnumerable<TimeTracker.Models.UserCategory>

@{
    ViewData["Title"] = "IndexMonth1";
}

<h1>Monthly Results</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Category.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MinutesSpent)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.Category.Title)</td>
        <td>@Html.DisplayFor(modelItem => item.MinutesSpent)</td>
    </tr>
}
    </tbody>
</table>
